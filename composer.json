{
    "name": "laravel-admin-ext/code-editor",
    "description": "Code editor extension for laravel-admin based on code-mirror",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "code mirror", "code", "editor"],
    "homepage": "https://github.com/laravel-admin-ext/code-editor",
    "license": "MIT",
    "authors": [
        {
            "name": "song",
            "email": "zosong@126.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\CodeEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Admin\\CodeEditor\\CodeEditorServiceProvider"
            ]
        }
    }
}
